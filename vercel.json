{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/[^.]+",
            "dest": "/"
        }
    ],
    "framework": "vite",
    "buildCommand": "npm run build"
}